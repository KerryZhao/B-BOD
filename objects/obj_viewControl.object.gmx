            <kind>1</kind>
            <string>///Update view based on players

//Get box around players
var minX = room_height;
var minY = room_height;
var maxX = 0;
var maxY = 0;
for(var i=0;i&lt;instance_number(obj_playerClient);i++){
    var player = instance_find(obj_playerClient, i);
    if(player.x&lt;minX)
        minX = player.x;
    if(player.y&lt;minY)
        minY = player.y;
    if(player.x+player.sprite_width&gt;maxX)
        maxX = player.x+player.sprite_width;
    if(player.y+player.sprite_height&gt;maxY)
        maxY = player.y+player.sprite_height;
}

//Get width of view based on players
var tempMinYSize = minYSize;
if(maxX-minX+bufferSize*2&gt;minXSize){
    view_wview[0] = maxX-minX+bufferSize*2;
    tempMinYSize = (maxX-minX)/minXSize*minYSize+bufferSize*2;
}
else
    view_wview[0] = minXSize;

//Get height of view based on players
if(maxY-minY+bufferSize*2&gt;tempMinYSize){
    view_hview[0] = maxY-minY+bufferSize*2;
    view_wview[0] = (maxY-minY)/minYSize*minXSize+bufferSize*2;
}
else
    view_hview[0] = tempMinYSize;

//Get x position of viewport based on players, viewport size, and room size
view_xview[0] = (minX+maxX-view_wview[0])/2;
if(view_wview[0]&gt;room_width)
    view_xview[0] = -(view_wview[0]-room_width)/2;
else if(view_xview[0]&lt;0)
    view_xview[0] = 0;
else if(view_xview[0]&gt;room_width-view_wview[0])
    view_xview[0] = room_width-view_wview[0];

//Get y position of viewport based on players, viewport size, and room size
view_yview[0] = (minY+maxY-view_hview[0])/2;
if(view_hview[0]&gt;room_height)
    view_yview[0] = -(view_hview[0]-room_height)/2;
else if(view_yview[0]&lt;0)
    view_yview[0] = 0;
else if(view_yview[0]&gt;room_height-view_hview[0])
    view_yview[0] = room_height-view_hview[0];
</string>
