        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw the key pressed or Backspace or Enter
var textLength = string_length(text);
if(keyboard_lastkey==vk_enter || keyboard_lastkey==vk_tab){
    if(enterButton!=-1){
        with(enterButton)
            event_perform(ev_mouse, ev_left_release);
    }
    else if(nextTextBox!=-1){
        focus = false;
        nextTextBox.focus = true;
    }
}
else if(keyboard_lastkey==vk_backspace &amp;&amp; textLength&gt;0){
    if(textPosition==textLength)
        text = string_copy(text, 0, textLength-1);
    else
        text = string_copy(text, 0, textPosition-1)+string_copy(text, textPosition+1, textLength);
    textPosition--;
}
else if(keyboard_lastkey==vk_left &amp;&amp; textPosition&gt;0){
    textPosition--;
}
else if(keyboard_lastkey==vk_right&amp;&amp; textPosition&lt;textLength){
    textPosition++;
}
else if(keyboard_lastchar!="" &amp;&amp; textLength&lt;textLimit &amp;&amp; keyboard_lastchar!="#"){
    if(textPosition==textLength)
        text += keyboard_lastchar;
    else
        text = string_copy(text, 0, textPosition)+keyboard_lastchar+string_copy(text, textPosition+1, textLength);
    textPosition++;
}
keyboard_lastchar = "";
keyboard_lastkey = -1;
</string>
          </argument>
        </arguments>
