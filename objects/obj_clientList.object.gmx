    var split = scr_splitString( global.multiplayerGame.clientInfo[| i], "&amp;");
    for(var j=0;j&lt;array_length_1d(split);j++)
        draw_text(x+j*colWidth, y+i*colHeight+colHeight, split[j]);
}

