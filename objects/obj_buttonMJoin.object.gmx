            <string>///Make sure an IP and name where entered
var name = "";
var ip = "";
for(var i=0;i&lt;instance_number(obj_textBox);i++){
    var textBox = instance_find(obj_textBox, i);
    if(textBox.isIP)
        ip = textBox.text;
    else
        name = textBox.text;
}

//Check IP
var split = scr_splitString(ip, ".");
var valid = array_length_1d(split)==4;
if(valid){
    for(var i=0;i&lt;array_length_1d(split) &amp;&amp; valid;i++){
        if(string_digits(split[i])==split[i]){
            var num = real(split[i]);
            if(floor(num)!=num || num&lt;0 || num&gt;255)
                valid = false;
        }
        else
            valid = false;
    }
}
if(!valid){
    show_message_async("Invaild IP Address!");
    exit;
}

//Check Name
if(string_lettersdigits(name)!=name || name==""){
    show_message("Your name can only contain letters and digits!");
    exit;
}
